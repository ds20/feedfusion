<ResourceDictionary  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  
<!--ToolBar-->
    <Style x:Key="ToolBar" TargetType="ToolBar">
      <Setter Property="Control.Background" >
        <Setter.Value>
          <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
            <LinearGradientBrush.RelativeTransform>
              <TransformGroup>
                <TranslateTransform X="0" Y="-0.5"/>
                <ScaleTransform ScaleX="0.86663196240259244" ScaleY="0.86663196240259244"/>
                <SkewTransform AngleX="0" AngleY="0"/>
                <RotateTransform Angle="-0.086873383355093717"/>
                <TranslateTransform X="0" Y="0.5"/>
                <TranslateTransform X="0.00065700495298732424" Y="-0.066684516882983269"/>
              </TransformGroup>
            </LinearGradientBrush.RelativeTransform>
            	<GradientStop Offset="0.391025641025641" Color="sc#1, 0.913098633, 0.9161102, 0.9559733"/>
		<GradientStop Offset="0.62820512820512819" Color="sc#1, 0.931295335, 0.9406383, 0.963822067"/>
		<GradientStop Offset="0" Color="sc#1, 0.4258481, 0.438258618, 0.5396918"/>
		<GradientStop Offset="1" Color="sc#1, 0.4198953, 0.43993634, 0.5174756"/>						
          </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>

    <!--ListBox-->
    <Style x:Key="ListBox" TargetType="ListBox">
      <Setter Property="SnapsToDevicePixels" Value="true"/>      
      <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto"/>
      <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"/>
      <Setter Property="ScrollViewer.CanContentScroll" Value="true"/>
      <Setter Property="Control.BorderBrush" Value="sc#1, 1, 1, 1"/>
      <Setter Property="Control.Background" >
        <Setter.Value>          
            <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
              <GradientStop Color="sc#1, 0.7140749, 0.7303383, 0.7250689" Offset="0.75000000769230771"/>
              <GradientStop Color="sc#1, 0.9816805, 0.9979439, 0.9933576" Offset="0.12820512820512819"/>
              <GradientStop Color="sc#1, 1, 1, 0.9705111" Offset="0.50000006923076927"/>
              <GradientStop Color="sc#1, 0.426, 0.438, 0.54" Offset="1"/>
              <GradientStop Color="sc#1, 0.7140749, 0.7303383, 0.7250689" Offset="0.25000074358974359"/>
              <GradientStop Color="sc#1, 0.426, 0.438, 0.54" Offset="0"/>
              <GradientStop Color="sc#1, 0.9816805, 0.9979439, 0.9933576" Offset="0.87179487179487181"/>
            </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>

    <!--ProgressBar-->
    <Style x:Key="ProgressBar" TargetType="ProgressBar">
      <Setter Property="Control.Foreground" >
        <Setter.Value>    
          
					<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
						<GradientStop Offset="0" Color="#FFABEDAC"/>
						<GradientStop Offset="0.51282051282051277" Color="#FF2ED331"/>
						<GradientStop Offset="1" Color="#FF8EE790"/>
					</LinearGradientBrush>
	   
        </Setter.Value>
      </Setter>
    </Style>
    
    <!--Brush-->
    <LinearGradientBrush x:Key="LightBrush" StartPoint="0,0" EndPoint="0,1">
     <GradientBrush.GradientStops>
       <GradientStopCollection>
         <GradientStop Offset="0" Color="sc#1, 0.880910039, 0.929700255, 0.8814985"/>
	 <GradientStop Offset="1" Color="sc#1, 0.426, 0.438, 0.54"/>
	 <GradientStop Offset="0.4358974358974359" Color="sc#1, 0.714, 0.73, 0.725"/>
       </GradientStopCollection>
     </GradientBrush.GradientStops>     
   </LinearGradientBrush>

    <!--TabControl-->
    <Style x:Key="TabControl" TargetType="TabControl">
      <Setter Property="Control.BorderBrush" Value="sc#1, 0.426, 0.438, 0.54"/>         
      <Setter Property="Control.Background" >
        <Setter.Value>
          <LinearGradientBrush StartPoint="0,0" EndPoint="0,1" SpreadMethod="Repeat" MappingMode="RelativeToBoundingBox">
            <GradientStop Offset="0" Color="sc#1, 0.714, 0.73, 0.725"/>
            <GradientStop Offset="1" Color="sc#1, 0.426, 0.438, 0.54"/>
          </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>

    <!--TabItem-->
    <Style x:Key="TabItem" TargetType="TabItem">
      <Setter Property="Control.BorderBrush" Value="sc#1, 0.426, 0.438, 0.54"/>
      <Setter Property="Control.Background" >
        <Setter.Value>
          <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
            <GradientStop Offset="0" Color="sc#1, 0.714, 0.73, 0.725"/>
            <GradientStop Offset="1" Color="sc#1, 0.426, 0.438, 0.54"/>
          </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>

    <!--StackPanel-->
    <Style x:Key="StackPanel" TargetType="StackPanel">
      <Setter Property="Control.BorderBrush" Setter.Value="sc#1, 0.426, 0.438, 0.54"/>
      <Setter Property="Control.Background" >
        <Setter.Value>
          <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
            <GradientStop Color="sc#1, 0.9953425, 0.997214732, 0.991605866" Offset="0"/>
            <GradientStop Color="sc#1, 0.426, 0.438, 0.54" Offset="1"/>
            <GradientStop Color="sc#1, 0.714, 0.73, 0.725" Offset="0.73717948717948722"/>
          </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>

    <!--TreeView-->
    <Style x:Key="TreeView" TargetType="TreeView">
      <Setter Property="Control.BorderBrush" Setter.Value="sc#1, 1, 1, 1"/>
      <Setter Property="Control.Background" >
        <Setter.Value>
          <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
            <GradientStop Color="sc#1, 0.9953425, 0.997214732, 0.991605866" Offset="0"/>
            <GradientStop Color="sc#1, 0.426, 0.438, 0.54" Offset="1"/>
            <GradientStop Color="sc#1, 0.714, 0.73, 0.725" Offset="0.73717948717948722"/>
          </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>
    
    <!--Label-->
    <Style x:Key ="Label" TargetType ="{x:Type Label}">
      <Setter Property ="Background" Value ="{DynamicResource LightBrush}"/>
    </Style>
    
    <!--Button-->
    <Style x:Key ="Button" TargetType ="{x:Type Button}">
      <Setter Property ="Background" Value ="{DynamicResource LightBrush}"/>
    </Style>

    <!--MenuItem-->
    <Style x:Key ="Item" TargetType ="{x:Type MenuItem}">
      <Setter Property ="Background" Value ="{DynamicResource LightBrush}"/>
    </Style>


    <!--Menu-->
  <Style x:Key="{x:Type Menu}" TargetType="{x:Type Menu}">
    <Setter Property="OverridesDefaultStyle" Value="True"/>
    <Setter Property="SnapsToDevicePixels" Value="True"/>
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type Menu}">
          <Border 
            Background="{DynamicResource LightBrush}"
            BorderBrush="{DynamicResource LightBrush}"
            BorderThickness="1">
            <StackPanel ClipToBounds="True" Orientation="Horizontal" IsItemsHost="True"/>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>

    <!--DockPanel-->
    <Style x:Key="DockPanel" TargetType="DockPanel">
      <Setter Property="Control.BorderBrush" Value="sc#1, 0.714, 0.73, 0.725"/>
      <Setter Property="Control.Background" Value="sc#1, 0.714, 0.73, 0.725"/>
    </Style>


   <!--Grid-->
    <Style x:Key="Grid" TargetType="Grid">
      <Setter Property="Control.BorderBrush" Value="sc#1, 0.714, 0.73, 0.725"/>
      <Setter Property="Control.Background" Value="sc#1, 0.714, 0.73, 0.725"/>
    </Style>

</ResourceDictionary>