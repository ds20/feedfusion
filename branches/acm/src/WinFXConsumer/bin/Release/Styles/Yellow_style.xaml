<ResourceDictionary  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  
<!--ToolBar-->
    <Style x:Key="ToolBar" TargetType="ToolBar">
      <Setter Property="Control.Background" >
        <Setter.Value>
          <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
            <LinearGradientBrush.RelativeTransform>
              <TransformGroup>
                <TranslateTransform X="0" Y="-0.5"/>
                <ScaleTransform ScaleX="0.86663196240259244" ScaleY="0.86663196240259244"/>
                <SkewTransform AngleX="0" AngleY="0"/>
                <RotateTransform Angle="-0.086873383355093717"/>
                <TranslateTransform X="0" Y="0.5"/>
                <TranslateTransform X="0.00065700495298732424" Y="-0.066684516882983269"/>
              </TransformGroup>
            </LinearGradientBrush.RelativeTransform>
            	<GradientStop Offset="0.391025641025641" Color="sc#1, 1, 0.9866344, 0.614895344"/>
		<GradientStop Offset="0.59615384615384615" Color="sc#1, 1, 0.996254861, 0.652869344"/>
		<GradientStop Offset="0" Color="sc#1, 1, 0.743184865, 0.210859656"/>
		<GradientStop Offset="1" Color="sc#1, 1, 0.743184865, 0.210859656"/>						
          </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>

    <!--ListBox-->
    <Style x:Key="ListBox" TargetType="ListBox">
      <Setter Property="SnapsToDevicePixels" Value="true"/>      
      <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto"/>
      <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"/>
      <Setter Property="ScrollViewer.CanContentScroll" Value="true"/>
      <Setter Property="Control.BorderBrush" Value="sc#1, 1, 1, 1"/>
      <Setter Property="Control.Background" >
        <Setter.Value>          
            <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
              <GradientStop Offset="0.29487179487179488" Color="sc#1, 1, 0.9866344, 0.614895344"/>
						<GradientStop Offset="0.71153846153846156" Color="sc#1, 1, 0.996254861, 0.652869344"/>
						<GradientStop Offset="0" Color="sc#1, 1, 0.743184865, 0.210859656"/>						
						<GradientStop Offset="0.53846153846153844" Color="sc#1, 1, 0.8216763, 0.452047825"/>
						<GradientStop Offset="1" Color="sc#1, 1, 0.743184865, 0.210859656"/>
	</LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>

    <!--ProgressBar-->
    <Style x:Key="ProgressBar" TargetType="ProgressBar">
      <Setter Property="Control.Foreground" >
        <Setter.Value>            
	<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
		<GradientStop Offset="0.391025641025641" Color="sc#1, 1, 0.743184865, 0.210859656"/>
		<GradientStop Offset="0.59615384615384615" Color="sc#1, 1, 0.743184865, 0.210859656"/>
		<GradientStop Offset="0" Color="sc#1, 1, 0.996254861, 0.652869344"/>
		<GradientStop Offset="1" Color="sc#1, 1, 0.996254861, 0.652869344"/>	
	</LinearGradientBrush>	   
        </Setter.Value>
      </Setter>
    </Style>
    
    <!--Brush-->
    <LinearGradientBrush x:Key="LightBrush" StartPoint="0,0" EndPoint="0,1">
     <GradientBrush.GradientStops>
       <GradientStopCollection>
         <GradientStop Offset="0" Color="sc#1, 1, 0.996254861, 0.652869344"/>
	 <GradientStop Offset="1" Color="sc#1, 1, 0.743184865, 0.210859656"/>
	 <GradientStop Offset="0.4358974358974359" Color="sc#1, 1, 0.9866344, 0.614895344"/>
       </GradientStopCollection>
     </GradientBrush.GradientStops>     
   </LinearGradientBrush>

    <!--TabControl-->
    <Style x:Key="TabControl" TargetType="TabControl">
      <Setter Property="Control.BorderBrush" Value="sc#1, 1, 0.743184865, 0.210859656"/>         
      <Setter Property="Control.Background" >
        <Setter.Value>
          <LinearGradientBrush StartPoint="0,0" EndPoint="0,1" SpreadMethod="Repeat" MappingMode="RelativeToBoundingBox">
            <GradientStop Offset="0" Color="sc#1, 1, 0.996254861, 0.652869344"/>
	 <GradientStop Offset="1" Color="sc#1, 1, 0.743184865, 0.210859656"/>
          </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>

    <!--TabItem-->
    <Style x:Key="TabItem" TargetType="TabItem">
      <Setter Property="Control.BorderBrush" Value="sc#1, 1, 0.743184865, 0.210859656"/>
      <Setter Property="Control.Background" >
        <Setter.Value>
          <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
            <GradientStop Offset="0" Color="sc#1, 1, 0.996254861, 0.652869344"/>
	 <GradientStop Offset="1" Color="sc#1, 1, 0.743184865, 0.210859656"/>
          </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>

    <!--StackPanel-->
    <Style x:Key="StackPanel" TargetType="StackPanel">
      <Setter Property="Control.BorderBrush" Setter.Value="sc#1, 0.426, 0.438, 0.54"/>
      <Setter Property="Control.Background" >
        <Setter.Value>
          <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
             <GradientStop Color="sc#1, 1, 0.743184865, 0.210859656" Offset="0"/>
            <GradientStop Color="sc#1, 1, 0.601272762, 0.0203486681" Offset="1"/>
            <GradientStop Color="sc#1, 0.9554194, 0.6849865, 0.207303524" Offset="0.532051282051282"/>
          </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>

    <!--TreeView-->
    <Style x:Key="TreeView" TargetType="TreeView">
      <Setter Property="Control.BorderBrush" Setter.Value="sc#1, 1, 1, 1"/>
      <Setter Property="Control.Background" >
        <Setter.Value>
          <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
             <GradientStop Color="sc#1, 1, 0.743184865, 0.210859656" Offset="0"/>
            <GradientStop Color="sc#1, 1, 0.459664822, 0.0203486681" Offset="1"/>
            <GradientStop Color="sc#1, 0.9554194, 0.612893343, 0.207303524" Offset="0.63461538461538458"/>
          </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>
    
    <!--Label-->
    <Style x:Key ="Label" TargetType ="{x:Type Label}">
      <Setter Property ="Background" Value ="{DynamicResource LightBrush}"/>
    </Style>
    
    <!--Button-->
    <Style x:Key ="Button" TargetType ="{x:Type Button}">
      <Setter Property ="Background" Value ="{DynamicResource LightBrush}"/>
    </Style>

    <!--MenuItem-->
    <Style x:Key ="Item" TargetType ="{x:Type MenuItem}">
      <Setter Property ="Background" Value ="{DynamicResource LightBrush}"/>
    </Style>


    <!--Menu-->
  <Style x:Key="{x:Type Menu}" TargetType="{x:Type Menu}">
    <Setter Property="OverridesDefaultStyle" Value="True"/>
    <Setter Property="SnapsToDevicePixels" Value="True"/>
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type Menu}">
          <Border 
            Background="{DynamicResource LightBrush}"
            BorderBrush="{DynamicResource LightBrush}"
            BorderThickness="1">
            <StackPanel ClipToBounds="True" Orientation="Horizontal" IsItemsHost="True"/>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>

    <!--DockPanel-->
    <Style x:Key="DockPanel" TargetType="DockPanel">
      <Setter Property="Control.BorderBrush" Value="sc#1, 1, 0.743184865, 0.210859656"/>
      <Setter Property="Control.Background" Value="sc#1, 1, 0.743184865, 0.210859656"/>
    </Style>


   <!--Grid-->
    <Style x:Key="Grid" TargetType="Grid">
      <Setter Property="Control.BorderBrush" Value="sc#1, 1, 0.743184865, 0.210859656"/>
      <Setter Property="Control.Background" Value="sc#1, 1, 0.743184865, 0.210859656"/>
    </Style>

</ResourceDictionary>