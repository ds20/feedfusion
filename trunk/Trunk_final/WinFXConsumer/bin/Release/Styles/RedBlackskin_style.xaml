      <Setter Property="Control.BorderBrush" Value="sc#1, 0.426, 0.438, 0.54"/>         
      <Setter Property="Control.Background" >
        <Setter.Value>
          <LinearGradientBrush StartPoint="0,0" EndPoint="0,1" SpreadMethod="Repeat" MappingMode="RelativeToBoundingBox">
            <GradientStop Offset="0.72435897435897434" Color="sc#1, 0.4908612, 0, 0.0104334969"/>													
							<GradientStop Offset="0" Color="sc#1, 1, 0, 0.0212554932"/>
							<GradientStop Offset="1" Color="sc#1, 0.394662231, 0, 0.00838874"/>
          </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>

    <!--TabItem-->
    <Style x:Key="TabItem" TargetType="TabItem">
      <Setter Property="Control.BorderBrush" Value="sc#1, 0.426, 0.438, 0.54"/>
      <Setter Property="Control.Background" >
        <Setter.Value>
          <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
            <GradientStop Offset="0.72435897435897434" Color="sc#1, 0.4908612, 0, 0.0104334969"/>													
							<GradientStop Offset="0" Color="sc#1, 1, 0, 0.0212554932"/>
							<GradientStop Offset="1" Color="sc#1, 0.394662231, 0, 0.00838874"/>
          </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>

    <!--StackPanel-->
    <Style x:Key="StackPanel" TargetType="StackPanel">
      <Setter Property="Control.BorderBrush" Setter.Value="sc#1, 0.426, 0.438, 0.54"/>
      <Setter Property="Control.Background" >
        <Setter.Value>
          <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
            <GradientStop Offset="0" Color="sc#1, 1, 0, 0.0116291046"/>
	    <GradientStop Offset="1" Color="sc#1, 0.184911475, 0, 0.003375426"/>
	    <GradientStop Offset="0.26923076923076922" Color="sc#1, 0.6762169, 0, 0.00527843041"/>
          </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>

    <!--TreeView-->
    <Style x:Key="TreeView" TargetType="TreeView">
      <Setter Property="Control.BorderBrush" Setter.Value="sc#1, 1, 1, 1"/>
      <Setter Property="Control.Background" >
        <Setter.Value>
          <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
            <GradientStop Offset="0.72435897435897434" Color="sc#1, 0.4908612, 0, 0.0104334969"/>													
							<GradientStop Offset="0" Color="sc#1, 1, 0, 0.0212554932"/>
							<GradientStop Offset="1" Color="sc#1, 0.394662231, 0, 0.00838874"/>
          </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>
    
    <!--Label-->
    <Style x:Key ="Label" TargetType ="{x:Type Label}">
      <Setter Property ="Background" Value ="{DynamicResource LightBrush}"/>
    </Style>
    
    <!--Button-->
    <Style x:Key ="Button" TargetType ="{x:Type Button}">
      <Setter Property ="Background" Value ="{DynamicResource LightBrush}"/>
    </Style>

    <!--MenuItem-->
    <Style x:Key ="Item" TargetType ="{x:Type MenuItem}">
      <Setter Property ="Background" Value ="{DynamicResource LightBrush}"/>
    </Style>


    <!--Menu-->
  <Style x:Key="{x:Type Menu}" TargetType="{x:Type Menu}">
    <Setter Property="OverridesDefaultStyle" Value="True"/>
    <Setter Property="SnapsToDevicePixels" Value="True"/>
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type Menu}">
          <Border 
            Background="{DynamicResource LightBrush}"
            BorderBrush="{DynamicResource LightBrush}"
            BorderThickness="1">
            <StackPanel ClipToBounds="True" Orientation="Horizontal" IsItemsHost="True"/>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>

    <!--DockPanel-->
    <Style x:Key="DockPanel" TargetType="DockPanel">
      <Setter Property="Control.BorderBrush" Value="sc#1,  0.185, 0, 0"/>
      <Setter Property="Control.Background" Value="sc#1, 0.676, 0, 0"/>
    </Style>


   <!--Grid-->
    <Style x:Key="Grid" TargetType="Grid">
      <Setter Property="Control.BorderBrush" Value="sc#1, 0.185, 0, 0"/>
      <Setter Property="Control.Background" Value="sc#1, 0.676, 0, 0"/>
    </Style>

</ResourceDictionary>