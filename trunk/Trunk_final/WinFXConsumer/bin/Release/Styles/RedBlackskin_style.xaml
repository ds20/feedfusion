         <GradientStop Offset="0.25641025641025639" Color="sc#1, 1, 0.08322334, 0.09388463"/>
					<GradientStop Offset="1" Color="sc#1, 0.184911475, 0, 0.003375426"/>
					<GradientStop Offset="0.50641025641025639" Color="sc#1, 0.6762169, 0, 0.00527843041"/>
