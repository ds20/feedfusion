    <!--ListBox-->
    <Style x:Key="ListBox" TargetType="ListBox">
      <Setter Property="SnapsToDevicePixels" Value="true"/>      
      <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto"/>
      <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"/>
      <Setter Property="ScrollViewer.CanContentScroll" Value="true"/>
      <Setter Property="Control.BorderBrush" Value="sc#1, 1, 1, 1"/>
      <Setter Property="Control.Background" >
        <Setter.Value>          
            <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
              <GradientStop Offset="0.32051282051282054" Color="sc#1, 0.403591454, 0, 0.008578535"/>
	        <GradientStop Offset="0.67307692307692313" Color="sc#1, 0.4542686, 0, 0.005381079"/>
		<GradientStop Offset="1" Color="sc#1, 1, 0, 0.0118451118"/>
		<GradientStop Offset="0" Color="sc#1, 1, 0, 0.0212554932"/>
		<GradientStop Offset="0.49358974358974361" Color="sc#1, 0.597370744, 0, 0.01269741"/>
	    </LinearGradientBrush>
        </Setter.Value>
      </Setter>
    </Style>

    <!--ProgressBar-->
    <Style x:Key="ProgressBar" TargetType="ProgressBar">
      <Setter Property="Control.Foreground" >
        <Setter.Value>    
          
					<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
						<GradientStop Offset="0" Color="sc#1, 1, 0.06587052, 0.07673361"/>
						<GradientStop Offset="0.51923076923076927" Color="sc#1, 1, 0, 0.01825428"/>
